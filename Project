    private void btnFindActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if(cn.GetConnection()==true){
                String dname="", dmanager="";
                boolean flag=false;
                try
                {
                rs = cn.GetRows("SELECT * FROM Department WHERE DeptCode ="+txtDeptCode.getText());
                cmbDeptManager.removeAllItems();
                
                while(rs.next())
                {
                    flag=true;
                
                    dname=rs.getString(2)+"";
                    dmanager=rs.getString(3)+"";
                }
                }catch(Exception ex)
                {
                    ex.printStackTrace();
                }
                if(flag==true)
                {
                    EnableDisable(true);
                    GetManager();
                    txtDeptName.setText(dname);
                    if(dmanager.equals("null"))
                        dmanager="";
                    cmbDeptManager.setSelectedItem(dmanager);
                }
                else
                {
                    EnableDisable(true);
                    GetManager();
                    txtDeptName.requestFocus();
                    
                }
//                JOptionPane.showMessageDialog(null,"The value exists","Project",JOptionPane.WARNING_MESSAGE);
            } else {
                txtDeptName.setEnabled(true);
                cmbDeptManager.setEnabled(true);
                
            }
    }                                       

    private void btnUpdateActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        
        if(cn.GetConnection()==true)
        {
            String sql;
            try
            {
                sql="UPDATE Department SET DeptName='"+txtDeptName.getText().toUpperCase()+"', DeptManager='"+cmbDeptManager.getSelectedItem().toString()+"' WHERE DeptCode="+txtDeptCode.getText();
            }catch(Exception ex)
            {
                sql="UPDATE Department SET DeptName='"+txtDeptName.getText().toUpperCase()+"', DeptManager=NULL WHERE DeptCode="+txtDeptCode.getText();
            }
            
            if(cn.executeDml(sql)==true)
            {
                 JOptionPane.showMessageDialog(null, "Department Updated ","Department",JOptionPane.INFORMATION_MESSAGE);
                 EnableDisable(false);
            }
            else
            {
                 JOptionPane.showMessageDialog(null, "Department Not Update","Department",JOptionPane.INFORMATION_MESSAGE);
                    EnableDisable(true);
            }
        }
    }                                         

    private void btnCloseActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        this.setVisible(false);
        new FileTrackingSystem().setVisible(true);
    }                                        

