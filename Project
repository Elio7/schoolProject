    private void btnNewActionPerformed(java.awt.event.ActionEvent evt) {                                       
        // TODO add your handling code here:
        txtDeptCode.requestFocus();
        btnSave.setEnabled(true);
        btnUpdate.setEnabled(true);
        ClearAll();
        txtDeptName.setEditable(false);
        cmbDeptManager.setEditable(false);
        GetManager();
    }                                      

    private void btnSaveActionPerformed(java.awt.event.ActionEvent evt) {                                        
        
        if (cn.GetConnection()==true)
        {
            
            //String sql="Insert into Department values ("+txtDeptCode.getText()+",'"+txtDeptName.getText()+"','"++"')";
            //cn.executeDml(sql);
            
            String sql = "";
            try
            {
                sql = "INSERT INTO Department VALUES("+txtDeptCode.getText()+",'"+txtDeptName.getText().toUpperCase()+"','"+cmbDeptManager.getSelectedItem().toString()+"')";
            }catch(Exception ex)
            {
                   sql = "INSERT INTO Department VALUES("+txtDeptCode.getText()+",'"+txtDeptName.getText().toUpperCase()+"',NULL)";
            }
            if(cn.executeDml(sql)==true)
            {
            //ps=
                    JOptionPane.showMessageDialog(null, "Department Added ","Department",JOptionPane.INFORMATION_MESSAGE);
                    EnableDisable(false);
            }
            else
            {
                    JOptionPane.showMessageDialog(null, "Department Not Added ","Department",JOptionPane.INFORMATION_MESSAGE);
                    EnableDisable(false);
            }
                            }
        
    }                                       

