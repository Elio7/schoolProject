    private void cmbEmpCodeActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        if(cn.GetConnection()==true) {
            try {
                rs = cn.GetRows("SELECT * FROM Vw_Managers");
                cmbEmpCode.removeAllItems();
//                cmbEmpCode.addItem(null);
                while(rs.next()){
                    cmbEmpCode.addItem(rs.getString(1));
                }
            } catch(Exception ex) {
                System.out.println(ex.getMessage());
            }
        } else {
            System.out.println("This code generates errors!!!");
        }
    }                                          

    private void txtDeptCodeKeyPressed(java.awt.event.KeyEvent evt) {                                       
        // TODO add your handling code here:
        //if(evt.getKeyCode() == 10) {
            try {
                if(cn.GetConnection()==true){
                    System.out.println("Connected to the database");
                    String dname="", dmanager="", sql="";
                    boolean flag=false;
                    try {
                        sql = "SELECT * FROM Department WHERE DeptCode = "+txtDeptCode.getText()+" ";
                        System.out.println(sql);
                        rs = cn.GetRows(sql);
                        cmbDeptManager.removeAllItems();

                        while(rs.next()) {
                            dname=rs.getString(2)+"";
                            dmanager=rs.getString(3)+"";
                            flag=true;
//                            System.out.println("Success");
                        }
                    }catch(Exception ex) {
                        ex.printStackTrace();
                        flag = false;
                    }
                    if(flag==true)
                    {
                        EnableDisable(true);
                        GetManager();
                        if(dmanager.equals("null"))
                        dmanager="";
                        txtDeptName.setText(dname);
                        cmbDeptManager.setSelectedItem(dmanager);
                    }
                    else
                    {
                        EnableDisable(true);
                        GetManager();
                        txtDeptName.requestFocus();

                    }
                    //                JOptionPane.showMessageDialog(null,"The value exists","Project",JOptionPane.WARNING_MESSAGE);
                } else {
                    txtDeptName.setEnabled(true);
                    cmbDeptManager.setEnabled(true);

                }
            } catch(Exception ex) {
                System.out.println(ex.getMessage());
            }
        /*}else{
              System.out.println("Error in key pressed");      
            }*/
    }                                      

    private void txtDeptCodeFocusLost(java.awt.event.FocusEvent evt) {                                      
        // TODO add your handling code here:
        
        if(cn.GetConnection()==true){
            String dname="", dmanager="";
            boolean flag=false;
            try
            {
                rs = cn.GetRows("SELECT * FROM Department WHERE DeptCode ="+txtDeptCode.getText());
                //                cmbDeptManager.removeAllItems();

                while(rs.next())
                {
                    flag=true;

                    dname=rs.getString(2)+"";
                    dmanager=rs.getString(3)+"";
                }
            }catch(Exception ex)
            {
                System.out.println(ex.getMessage());
                //                    ex.printStackTrace();
            }
            if(flag==true)
            {
                EnableDisable(true);
                GetManager();
                txtDeptName.setText(dname);
                cmbDeptManager.setSelectedItem(dmanager);

            }
            else
            {
                EnableDisable(true);
                GetManager();
                txtDeptName.setText("");
                txtDeptName.requestFocus();

            }
            //                JOptionPane.showMessageDialog(null,"The value exists","Project",JOptionPane.WARNING_MESSAGE);
        } else {
            txtDeptCode.setEnabled(true);
            cmbDeptManager.setEnabled(false);
            JOptionPane.showMessageDialog(null, "Unable to connect to database", "Department", JOptionPane.ERROR_MESSAGE);

        }
        txtDeptCode.requestFocus(true);

    }                                     

