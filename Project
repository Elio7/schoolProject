    private void getAdminEmployees()
    {
        if(cn.GetConnection()==true)
        {
            try
            {
                rs=cn.GetRows("SELECT EmployeeId FROM Employee WHERE DeptId = (SELECT DeptCode FROM Department WHERE DeptName='ADMIN')");
                while(rs.next())
                {
//                    cmbEmployeeCode.addItem(rs.getInt(1)+"");
                }
            }catch(Exception ex){}
        }
    }
     public void keyPressed(KeyEvent e) {
                int keyCode = e.getKeyCode();
                System.out.println(keyCode);
            }
    
    private void GetManager()
    {
        if(cn.GetConnection()==true)
        {
            try
            {
                rs = cn.GetRows("SELECT * FROM Vw_Managers");
                cmbDeptManager.removeAllItems();
                cmbDeptManager.addItem(null);
                while(rs.next())
                {
                    cmbDeptManager.addItem(rs.getString(1));
                }
            }catch(Exception ex)
            {
                ex.printStackTrace();
            }
        }
    }
    
    private void ClearAll()
    {
        txtDeptCode.setText("");
        txtDeptName.setText("");
        
    }
    
    private void EnableDisable(boolean flag)
    {
        txtDeptName.setEnabled(flag);
        cmbDeptManager.setEditable(flag);
    }
